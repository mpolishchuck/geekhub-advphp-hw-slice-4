{% extends "PaulMaxwellStudentsAccountingBundle::layout.html.twig" %}

{% block content %}
    {% if top_msg is defined %}
        <p>{{ top_msg }}</p>
    {% endif %}
    {% if criteria is defined %}
        <ul class="list-unstyled">
            {% for criterion in criteria %}
                <li>{{ criterion }}</li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}
